# Problem: Counting DNA Nucleotides
# Function to count nucleotides in a DNA string
def count_nucleotides(dna_sequence):
    count_A = dna_sequence.count('A')
    count_C = dna_sequence.count('C')
    count_G = dna_sequence.count('G')
    count_T = dna_sequence.count('T')

    # Return the counts in the order A, C, G, T
    return count_A, count_G, count_T, count_C

#Example Sequence:
dna_sequence = "ACACGTCGTACGTACGTGATC"

#Call the function and print the result
result = count_nucleotides(dna_sequence)
print(result)
